{
  "name": "spec-3nweb-server",
  "version": "1.6.1",
  "description": "3NWeb spec server.",
  "main": "build/lib.js",
  "types": "build/lib.d.ts",
  "scripts": {
    "build": "tsc -p ts-code",
    "test": "node build/tests/jasmine.js",
    "clear-for-packing": "npm prune --production && bash -c \"rm -rf build/mock build/tests 'node_modules/ecma-nacl/c sources'\"",
    "clear-for-packing-mock": "npm prune --production && bash -c \"rm -rf build/tests 'node_modules/ecma-nacl/c sources'\"",
    "pack-deb": "npm run clear-for-packing && bash packing/deb/create-bin.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3nsoft/spec-server.git"
  },
  "keywords": [
    "3NWeb",
    "server",
    "ASMail",
    "MailerId",
    "3NStorage"
  ],
  "author": {
    "name": "3NSoft Inc.",
    "url": "https://3nsoft.com",
    "email": "hq@3nsoft.com"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "ecma-nacl": "^2.4.0",
    "express": "^4.18.0",
    "punycode": "^2.1.1",
    "ws": "^7.4.6",
    "xsp-files": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jasmine": "^3.9.1",
    "@types/node": "^16.11.7",
    "@types/ws": "^4.0.0",
    "jasmine": "^3.9.0",
    "rxjs": "^5.3.0",
    "typescript": "^4.7.4",
    "xhr2": "^0.1.3"
  },
  "readmeFilename": "README.md"
}
